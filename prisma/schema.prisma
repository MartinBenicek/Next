generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model games {
  id         Int      @id @default(autoincrement())
  TypHry     String
  Hlaseno    String?
  UhraneBody Int?
  Cena       Int
  created    DateTime @default(now())
}
